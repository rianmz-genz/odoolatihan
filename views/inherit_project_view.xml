<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
    
        <record id="edit_project_inherit" model="ir.ui.view">
          <field name="name">project.edit.project.inherit</field>
          <field name="model">project.project</field>
          <field name="inherit_id" ref="project.edit_project" />
          <field name="arch" type="xml">
                <page name="settings" position="after">
                     <page name="anggaran" string="Anggaran">
                        <group>
                            <group>
                                <field name="anggaran"></field>
                                <field name="persenan"></field>
                                <field name="backdoor_salary"></field>
                            </group>
                        </group>
                        <field name="keyholder_ids">
                                    <tree editable="top">
                                            <field name="nama"></field>
                                            <field name="jumlah_bagian"></field>
                                            <field name="posisi_id"></field>
                                            <field name="bagian_persen"></field>
                                    </tree>
                        </field>
                     </page>
                </page>
          </field>
        </record>

    </data>
</odoo>
